{
    "version": "2.0",
    "logging": {
      "logLevel": {
        "default": "Information",
        "Host.Results": "Information",
        "Function": "Information",
        "Worker": "Information"
      },
      "applicationInsights": {
        "samplingSettings": {
          "isEnabled": true,
          "maxTelemetryItemsPerSecond": 5
        }
      }
    },
    "extensions": {
      "http": {
        "routePrefix": "api"
      }
    }
  }